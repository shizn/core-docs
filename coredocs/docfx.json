{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "corefx/src/**/ref/**/project.json"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd": ".."
        }
      ],
      "dest": "api"
    },
    {
      "src": [
        {
          "files": [
            "corefx/src/**/src/**/project.json",
            "coreclr/src/mscorlib/mscorlib.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**"],
          "cwd": ".."
        }
      ],
      "dest": "api_src"
    }
  ],
  "build": {
    "content":
      [
        {
          "files": ["api/**/*.yml"]
        },
        {
          "files": [
            "docs/**/*.md",
            "api/*.md"
          ]
        },
        {
          "files": ["toc.yml", "index.md"]
        }
      ],
    "resource":
      [
        {
          "files": [
            "docs/images/**",
            "images/**",
            "_themes/**"
          ],
          "exclude": [
          "**/obj/**",
          "_themes/DocPacker/**"
          ]
        }
      ],
    "overwrite": "apidoc/*.md",
    "externalReference": [
    ],
    "globalMetadata": {
      "breadcrumb_path": "/qinen/coredocs-demo/toc.json",
      "_appTitle": "CoreCLR site"
    },
    "dest": "coredocs",
    "template": ["docs.html"]
  }
}
